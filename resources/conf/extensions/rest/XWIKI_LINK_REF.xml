<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE swatch SYSTEM "swatch.dtd">

<swatch name="XWIKI_LINK_REF">
    <!-- Base Satellite definition -->
    <sat shape="edgeShp"
         isBack="true" isVisible="true" isSuper="true" isSub="false" isCur="false" isRest="true" isTip="false" isNoSided="false">
        <slice inCol="255 157 0" outCol="238 242 245" alpha="1" linewidth="1" /> 
    </sat>

    <!-- General purpose Selection -->
    <!-- Sel sats must be declared before the ones they should hides when they'll be activated -->
    <sat shape="edgeShp" linkDarkCol="128 0 0" linkNormCol="255 0 0" linkLitCol="255 143 143" selection="search" transfo="noTrf"
         isVisible="true" isSuper="true" isSub="false" isCur="false" isRest="true" isBack="false" isTip="false" isNoSided="false">
        <slice inCol="255 0 0" alpha="1" linewidth="1" />
    </sat>

    <!-- References -->
    <shape id="edgeShp" scale="1" anchor="center" />
    <transfo id="noTrf" scale="0" map="absolute" />
</swatch>
